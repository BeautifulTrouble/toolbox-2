{
  "name": "beautiful-trouble-2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "yarn git-submodule && yarn wp-link && yarn wp-cli-install",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "wp": "cd wordpress && php wp-cli.phar",
    "wp-cli-install": "cd wordpress && curl -O https://raw.githubusercontent.com/wp-cli/builds/gh-pages/phar/wp-cli.phar",
    "wp-link": "cd wordpress && rm -rf {wp-content,wp-config.php} && ln -s ../wp-content && ln -s ../wp-config.php",
    "git-submodule": "git submodule init && git submodule update"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^2.6.5",
    "headroom.js": "^0.11.0",
    "local-storage-fallback": "^4.1.1",
    "showdown": "^1.9.1",
    "showdown-target-blank": "^1.0.2",
    "vue": "^2.6.10",
    "vue-lazy-image-loading": "^1.2.1",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.0.3",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-svg-inline-loader": "^1.5.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
